<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center">


    <Button
        android:id="@+id/btnGo"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_above="@+id/btnbk"
        android:layout_alignParentStart="true"
        android:background="#8B4513"
        android:text="About Me"
        android:textSize="25dp"
        android:textColor="#FFFFFF"/>


    <Button
        android:id="@+id/btnbk"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/image"
        android:background="#00FF66"
        android:text="Home"
        android:textSize="25dp"
        android:textColor="#FFFFFF"/>

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="200px"
        android:layout_height="200px"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="93dp"
        android:layout_marginTop="17dp"
        app:srcCompat="@drawable/a01" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="200px"
        android:layout_height="200px"
        android:layout_above="@+id/imageView5"
        android:layout_toEndOf="@+id/imageView1"
        app:srcCompat="@drawable/a02" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="200px"
        android:layout_height="200px"
        android:layout_alignTop="@+id/imageView2"
        android:layout_toEndOf="@+id/imageView5"
        app:srcCompat="@drawable/a03" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="200px"
        android:layout_height="200px"
        android:layout_below="@+id/imageView1"
        android:layout_toStartOf="@+id/imageView5"
        app:srcCompat="@drawable/a04" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="200px"
        android:layout_height="200px"
        android:layout_below="@+id/imageView1"
        android:layout_toEndOf="@+id/imageView1"
        app:srcCompat="@drawable/a05" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="200px"
        android:layout_height="200px"
        android:layout_below="@+id/imageView3"
        android:layout_toEndOf="@+id/imageView5"
        app:srcCompat="@drawable/a06" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="200px"
        android:layout_height="200px"
        android:layout_below="@+id/imageView4"
        android:layout_toStartOf="@+id/imageView8"
        app:srcCompat="@drawable/a07" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="200px"
        android:layout_height="200px"
        android:layout_below="@+id/imageView5"
        android:layout_toEndOf="@+id/imageView4"
        app:srcCompat="@drawable/a08" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="200px"
        android:layout_height="200px"
        android:layout_below="@+id/imageView6"
        android:layout_toEndOf="@+id/imageView8"
        app:srcCompat="@drawable/a09" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="200px"
        android:layout_height="200px"
        android:layout_alignStart="@+id/imageView7"
        android:layout_below="@+id/imageView7"
        app:srcCompat="@drawable/a10" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="200px"
        android:layout_height="200px"
        android:layout_below="@+id/imageView7"
        android:layout_toEndOf="@+id/imageView7"
        app:srcCompat="@drawable/a11" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="200px"
        android:layout_height="200px"
        android:layout_below="@+id/imageView9"
        android:layout_toEndOf="@+id/imageView11"
        app:srcCompat="@drawable/a12" />

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="200px"
        android:layout_height="200px"
        android:layout_alignStart="@+id/imageView10"
        android:layout_below="@+id/imageView10"
        app:srcCompat="@drawable/a13" />


</RelativeLayout>
